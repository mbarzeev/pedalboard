{
    "name": "@pedalboard/stylelint-plugin-craftsmanlint",
    "version": "1.3.7",
    "description": "A set of StyleLint rules for your code craftsmanship",
    "main": "./dist/cjs/index.js",
    "types": "./dist/types/index.d.ts",
    "author": "Matti Bar-Zeev",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/mbarzeev/pedalboard.git",
        "directory": "packages/stylelint-plugin-craftsmanlint"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ],
    "exports": {
        "require": "./dist/cjs/index.js",
        "default": "./dist/cjs/index.js"
    },
    "scripts": {
        "test": "jest",
        "lint": "eslint ./src",
        "build": "tsc --project tsconfig.cjs.json",
        "ts-coverage": "typescript-coverage-report"
    },
    "typeCoverage": {
        "ignoreFiles": [
            "jest.config.ts"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.16.5",
        "@babel/preset-env": "^7.16.11",
        "csstype": "^3.1.1",
        "eslint": "8.57.0",
        "jest": "^27.4.3",
        "jest-preset-stylelint": "7.0.0",
        "ts-node": "^10.9.2",
        "typescript": "^4.6.4",
        "typescript-coverage-report": "^0.7.0"
    },
    "dependencies": {
        "stylelint": "15.11.0"
    }
}
